createButton("Kitchen Gun", "https://raw.githubusercontent.com/Nicolasbarbosa321/musics/refs/heads/main/SB%20Shotgun.txt", "", tabFrames["Painel"])
createButton("Ak-47", "https://raw.githubusercontent.com/Nicolasbarbosa321/musics/refs/heads/main/SB%20Shotgun.txt", "", tab
